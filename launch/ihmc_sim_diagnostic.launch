<launch>
  <param name="use_sim_time" value="true"/>
  <arg name="ihmc_network_file" default="./IHMCNetworkParameters.ini" />
  <arg name="description_model" default="$(find ihmc_msgs)/description/atlas_unplugged_v5_no_hands.urdf" />
  <param name="/atlas/robot_description" textfile="$(arg description_model)" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" >
    <remap from="joint_states" to="/ihmc_msgs/atlas/output/joint_states" />
    <remap from="robot_description" to="/atlas/robot_description" />
  </node>
  <node name="IHMCAtlasAPISimulator" pkg="ihmc_msgs" type="IHMCAtlasAPISimulator" args="--requestAutomaticDiagnostic" required="true" output="screen" cwd="node">
	<env name="IHMC_ATLAS_API_OPTS" value="-DnetworkParameterFile=$(arg ihmc_network_file)" />
  </node>
</launch>
